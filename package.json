{
  "name": "walking_tracker",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "run-android": "react-native run-android --mode=Debug",
    "build-android": "react-native build-android --mode=Release",
    "clean": "del-cli android/build android/app/.cxx ios/build",
    "clear": "react-native clean --include watchman,yarn,metro,android",
    "doctor": "react-native doctor --fix",
    "ios": "react-native run-ios --scheme walking_tracker",
    "run-ios": "yarn ios --configuration Debug",
    "build-ios": "yarn ios --configuration Release",
    "lint": "eslint .",
    "patch": "patch-package",
    "pods": "(cd ios && pod install --no-repo-update)",
    "postandroid": "yarn unwatch",
    "postinstall": "yarn patch && yarn pods",
    "postios": "yarn unwatch",
    "preandroid": "(cd android && ./gradlew clean)",
    "preios": "yarn pods",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "unwatch": "watchman watch-del \"$PWD\"",
    "watch": "tsc --noEmit --watch"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@dongminyu/react-native-step-counter": "^0.1.14",
    "@dongminyu/segway-ble-manager": "^0.1.3",
    "react": "18.2.0",
    "react-native": "^0.71.4",
    "react-native-permissions": "^3.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.21.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.5.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "0.76.0",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.4",
    "react-native-flipper": "^0.183.0",
    "react-native-gradle-plugin": "^0.71.16",
    "react-test-renderer": "18.2.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "reactNativePermissionsIOS": [
    "Motion",
    "Bluetooth"
  ]
}
