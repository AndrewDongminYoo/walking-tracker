{
  "name": "walking_tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "poststart": "watchman watch-del \"$PWD\" ; watchman watch-project \"$PWD\"",
    "android": "react-native run-android",
    "ios": "react-native run-ios --device --terminal vscode",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "bundle:android": "react-native bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/main.jsbundle --assets-dest android/app/src/main/res/",
    "bundle:ios": "react-native bundle --platform ios --entry-file index.js --bundle-output ios/walking_tracker/main.jsbundle --assets-dest ios/walking_tracker/Images.xcassets/",
    "postinstall": "(npx patch-package; npx pod-install --repo-update)",
    "preios": "(npx patch-package; npx pod-install)",
    "doctor": "react-native doctor --fix",
    "clear": "react-native clean --include watchman,yarn,metro,android"
  },
  "dependencies": {
    "@andrewdongminyoo/react-native-step-counter": "*",
    "react": "18.2.0",
    "react-native": "0.71.1",
    "react-native-permissions": "^3.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.74.1",
    "prettier": "^2.4.1",
    "react-native-flipper": "^0.177.0",
    "react-native-gradle-plugin": "^0.71.13",
    "react-test-renderer": "18.2.0",
    "typescript": "4.9.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
