{
  "name": "walking_tracker",
  "version": "0.0.2",
  "repository": "https://github.com/AndrewDongminYoo/walking_tracker",
  "author": "Dongmin,Yoo <ydm2790@gmail.com> (https://github.com/AndrewDongminYoo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AndrewDongminYoo/walking_tracker/issues"
  },
  "scripts": {
    "android": "react-native run-android",
    "run-android": "react-native run-android --mode=Debug",
    "build-android": "react-native build-android --mode=Release",
    "clean": "del-cli android/build android/app/.cxx ios/build",
    "clear": "react-native clean --include watchman,yarn,metro,android",
    "doctor": "react-native doctor --fix",
    "ios": "react-native run-ios --scheme walking_tracker",
    "run-ios": "yarn ios --configuration Debug",
    "build-ios": "yarn ios --configuration Release",
    "lint": "eslint . --ext .ts --ext .tsx --fix",
    "format": "prettier \"**/*.{ts,js,jsx,tsx}\" --check --write",
    "pods": "(cd ios && pod install --no-repo-update)",
    "postandroid": "yarn unwatch",
    "postinstall": "yarn rnx-align-deps --write && yarn patch-package && yarn pods",
    "postios": "yarn unwatch",
    "preandroid": "(cd android && ./gradlew clean)",
    "preios": "yarn pods",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "unwatch": "watchman watch-del \"$PWD\"; watchman watch-project \"$PWD\"",
    "watch": "tsc --noEmit --watch"
  },
  "engines": {
    "node": ">= 16.0.0 || >= 18.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@dongminyu/react-native-step-counter": "^0.2.2",
    "react": "18.2.0",
    "react-native": "^0.71.0",
    "react-native-circular-progress-indicator": "^4.4.2",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-permissions": "^3.10.0",
    "react-native-reanimated": "^2.14.1",
    "react-native-svg": "^13.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@commitlint/config-conventional": "^17.8.0",
    "@fluentui/token-pipeline": "^0.21.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-community/eslint-plugin": "^1.3.0",
    "@rnx-kit/align-deps": "^2.2.5",
    "@rnx-kit/jest-preset": "^0.1.15",
    "@rnx-kit/tools-language": "^2.0.1",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.5",
    "@types/react-test-renderer": "^18.0.3",
    "commitlint": "^17.8.0",
    "cosmiconfig": "8.3.6",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.73.7",
    "patch-package": "^8.0.0",
    "postman-jsdoc-theme": "^0.0.3",
    "prettier": "^3.0.3",
    "react-native-flipper": "^0.212.0",
    "react-native-gradle-plugin": "^0.71.19",
    "react-test-renderer": "18.2.0",
    "typescript": "5.2.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "reactNativePermissionsIOS": [
    "Motion",
    "Bluetooth"
  ],
  "keywords": [
    "activity-recognition-api",
    "activity-recognition",
    "android",
    "core-motion",
    "ios",
    "motion-sensor-api",
    "motion-sensor",
    "pedometer-api",
    "pedometer",
    "react-native-step-counter",
    "react-native",
    "sensor-manager",
    "step-counter-api",
    "step-counter",
    "step-detection-api",
    "step-detection"
  ],
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.71"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "core-android",
        "core-ios",
        "core/testing",
        "gestures",
        "react",
        "svg"
      ]
    }
  }
}
